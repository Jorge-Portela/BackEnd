package sintaxe_variaveis_e_fluxo;

public class Rascunho {
	public static void main(String[] args) {
		
		double salario = 3300.0;
		
		if (salario >= 1900.0 && salario <= 2800.0) {
			
			System.out.println("A alíquota é 7.5%");
			System.out.println("Deduzir valor de R$ 142.00");
		} else if (salario >= 2800.01 && salario <= 3751.0) {
			
			System.out.println("Alíquota de 15% ");
			System.out.println("Deduzir valor de R$ 350.00");
		} else if(salario >= 3751.01 && salario <= 4664.00){
			
			System.out.println("Alíquota de 22.5% ");
			System.out.println("Deduzir valor de R$ 636.00");
			
		}
	}

}
