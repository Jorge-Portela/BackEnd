package ponto_flutuante;

public class TestaPontoFlutuante {
	
	public static void main(String[] args) {
		
		double salario;
		salario = 1800.78;
		
		System.out.println("Meu salário é R$" + salario);
		
		double altura;
		altura = 1.80 + 2;
		
		System.out.println(altura);
		
		double medida;
		medida = 5 / 2.0;
		
		System.out.println(medida);
		
		
	}

}
